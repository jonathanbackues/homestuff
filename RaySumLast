package Unit8;
public class RaySumLast
{
	static int sum;
	public static int go(int[] ray)
	{
		for(int i=0;i<ray.length;i++)
		{
			if (ray[i]>ray[ray.length-1]) {
				sum += ray[i];
			}
			if (sum == 0) {
				sum = -1;
			}
		}
		return sum;
	}
}
